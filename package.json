{
  "name": "node-typescript-starter",
  "version": "1.0.0",
  "description": "Node typescript starter project ",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:fe\" \"npm run dev:be\"",
    "build": "npm run build:fe && npm run build:be",
    "start": "NODE_ENV=production node dist/server.js",
    "dev:fe": "webpack -w --env.NODE_ENV=development",
    "dev:be": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"tsc -w\" \"NODE_ENV=development nodemon --delay 1500ms dist/server.js\"",
    "build:fe": "webpack --env.NODE_ENV=production --progress",
    "build:be": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parkoon/node-typescript-starter.git"
  },
  "keywords": [
    "node.js",
    "express",
    "handlebars",
    "typescript"
  ],
  "author": "parkoon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/parkoon/node-typescript-starter/issues"
  },
  "homepage": "https://github.com/parkoon/node-typescript-starter#readme",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/helmet": "0.0.46",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lusca": "^1.6.1",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/express-handlebars": "^3.1.0",
    "@types/shelljs": "^0.8.7",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.3",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lusca": "^1.6.1",
    "morgan": "^1.10.0",
    "shelljs": "^0.8.4",
    "xss-clean": "^0.1.1"
  }
}
