{
    "name": "node-typescript-starter",
    "version": "1.0.0",
    "description": "Boilerplate to quick-start Node.js development in TypeScript.",
    "main": "server.js",
    "scripts": {
        "dev": "concurrently \"webpack -w --env.NODE_ENV=development\" \"NODE_ENV=development nodemon\"",
        "build": "concurrently \"webpack --env.NODE_ENV=production --progress\" \"tsc\"",
        "start": "NODE_ENV=production node build/src/server.js",
        "clean": "rimraf build"
    },
